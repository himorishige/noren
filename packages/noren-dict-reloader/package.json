{
  "name": "@himorishige/noren-dict-reloader",
  "version": "2.0.0",
  "description": "Dynamic dictionary and policy hot-reloading for Noren using ETag-based updates",
  "keywords": [
    "pii",
    "privacy",
    "redaction",
    "hot-reload",
    "dictionary",
    "etag",
    "dynamic-config",
    "noren-plugin"
  ],
  "homepage": "https://github.com/himorishige/noren#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/himorishige/noren.git",
    "directory": "packages/noren-dict-reloader"
  },
  "bugs": {
    "url": "https://github.com/himorishige/noren/issues"
  },
  "author": "himorishige",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=20.10.0"
  },
  "files": [
    "dist",
    "example"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "@types/node": "^20"
  },
  "peerDependencies": {
    "@himorishige/noren-core": "workspace:*"
  }
}
